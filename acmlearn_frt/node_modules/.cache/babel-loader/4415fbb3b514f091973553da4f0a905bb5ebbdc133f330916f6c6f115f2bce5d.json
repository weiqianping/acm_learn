Names[courseIndex].splice(chapterIndex, 1);\r\n      },\r\n      handleEditVideo(courseIndex, chapterIndex, videoIndex) {\r\n        // 处理编辑视频逻辑\r\n        console.log('编辑视频', courseIndex, chapterIndex, videoIndex);\r\n      },\r\n      handleDeleteVideo(courseIndex, chapterIndex, videoIndex) {\r\n        // 处理删除视频逻辑\r\n        this.courses[courseIndex].chapters[chapterIndex].videos.splice(videoIndex, 1);\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  /* 添加一些样式以美化组件 */\r\n  .el-collapse {\r\n    width: 80%;\r\n    margin: 0 auto;\r\n  }\r\n  \r\n  .el-button--text {\r\n    margin-left: 10px;\r\n  }\r\n  \r\n  ul {\r\n    list-style-type: none;\r\n    padding: 0;\r\n  }\r\n  \r\n  li {\r\n    margin: 10px 0;\r\n  }\r\n  </style>"],"mappings":";AAmCE,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,kBAAkB,EAAE,CAAC,CAAC;MACtBC,OAAO,EAAE,CACP;QACEC,IAAI,EAAE,KAAK;QACXC,QAAQ,EAAE,CACR;UACED,IAAI,EAAE,KAAK;UACXE,MAAM,EAAE,CACN;YAAEF,IAAI,EAAE;UAAM,CAAC,EACf;YAAEA,IAAI,EAAE;UAAM,CAAC;QAEnB,CAAC,EACD;UACEA,IAAI,EAAE,KAAK;UACXE,MAAM,EAAE,CACN;YAAEF,IAAI,EAAE;UAAM,CAAC,EACf;YAAEA,IAAI,EAAE;UAAM,CAAC;QAEnB,CAAC;MAEL,CAAC,EACD;QACEA,IAAI,EAAE,KAAK;QACXC,QAAQ,EAAE,CACR;UACED,IAAI,EAAE,KAAK;UACXE,MAAM,EAAE,CACN;YAAEF,IAAI,EAAE;UAAM,CAAC,EACf;YAAEA,IAAI,EAAE;UAAM,CAAC;QAEnB,CAAC;MAEL,CAAC;IAEL,CAAC;EACH,CAAC;EACDG,OAAO,EAAE;IACPC,eAAeA,CAAA,EAAG;MAChB;MACA,MAAMC,SAAQ,GAAI;QAChBL,IAAI,EAAE,MAAM,IAAI,CAACD,OAAO,CAACO,MAAK,GAAI,CAAC,EAAE;QACrCL,QAAQ,EAAE;MACZ,CAAC;MACD,IAAI,CAACF,OAAO,CAACQ,IAAI,CAACF,SAAS,CAAC;MAC5B,IAAI,CAACR,WAAW,CAACU,IAAI,CAAC,IAAI,CAACR,OAAO,CAACO,MAAK,GAAI,CAAC,CAAC;IAChD,CAAC;IACDE,gBAAgBA,CAACC,WAAW,EAAE;MAC5B;MACA,MAAMC,UAAS,GAAI;QACjBV,IAAI,EAAE,MAAM,IAAI,CAACD,OAAO,CAACU,WAAW,CAAC,CAACR,QAAQ,CAACK,MAAK,GAAI,CAAC,EAAE;QAC3DJ,MAAM,EAAE;MACV,CAAC;MACD,IAAI,CAACH,OAAO,CAACU,WAAW,CAAC,CAACR,QAAQ,CAACM,IAAI,CAACG,UAAU,CAAC;MACnD,IAAI,CAAC,IAAI,CAACZ,kBAAkB,CAACW,WAAW,CAAC,EAAE;QACzC,IAAI,CAACE,IAAI,CAAC,IAAI,CAACb,kB